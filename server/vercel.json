{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "CLIENT_ORIGIN": "https://cloudnotesbykush.vercel.app",
    "SAMPLE_MODE": "false",
    "NODE_ENV": "production"
  }
}
